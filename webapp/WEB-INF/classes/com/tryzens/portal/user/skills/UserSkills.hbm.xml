<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tryzens.portal.user.skills.UserSkills" table="SKILLS_USER"
		catalog="tryzens_portal" lazy="false">
		<cache usage="read-write"/>

		<id name="id" type="long">
			<column name="SKILLS_USER_ID" />
			<generator class="identity" />
		</id>

		<property name="rating" type="long">
			<column name="RATING" length="1" not-null="true" />
		</property>

		<property name="lastUpdate" type="timestamp">
			<column name="LASTUPDATE_DATE" length="19" />
		</property>

		<many-to-one name="user" column="USER_ID"
			class="com.tryzens.portal.user.User" embed-xml="false" not-null="false" />
		<many-to-one name="skill" column="SKILL_ID"
			class="com.tryzens.portal.user.skills.Skill" embed-xml="false" not-null="false"/>

	</class>
</hibernate-mapping>